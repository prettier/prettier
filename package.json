{
  "name": "prettier",
  "version": "3.8.0-dev",
  "description": "Prettier is an opinionated code formatter",
  "bin": "./bin/prettier.cjs",
  "repository": "prettier/prettier",
  "funding": "https://github.com/prettier/prettier?sponsor=1",
  "homepage": "https://prettier.io",
  "author": "James Long",
  "type": "module",
  "license": "MIT",
  "main": "./src/index.cjs",
  "browser": "./standalone.js",
  "unpkg": "./standalone.js",
  "imports": {
    "#universal": {
      "browser": "./src/universal/index.browser.js",
      "default": "./src/universal/index.js"
    },
    "#universal/*": {
      "browser": "./src/universal/*.browser.js",
      "default": "./src/universal/*.js"
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "require": "./src/index.cjs",
      "default": "./src/index.js"
    },
    "./standalone": "./src/standalone.js",
    "./plugins/*": "./src/plugins/*.js",
    "./*": "./*"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "index.js",
    "standalone.js",
    "src",
    "bin"
  ],
  "dependencies": {
    "@angular/compiler": "21.0.2",
    "@babel/code-frame": "8.0.0-beta.3",
    "@babel/parser": "8.0.0-beta.3",
    "@babel/types": "8.0.0-beta.3",
    "@glimmer/syntax": "0.95.0",
    "@prettier/cli": "0.10.0",
    "@prettier/html-event-attributes": "0.0.1",
    "@prettier/html-tags": "2.0.0",
    "@prettier/parse-srcset": "3.1.0",
    "@typescript-eslint/typescript-estree": "8.48.1",
    "@typescript-eslint/visitor-keys": "8.48.1",
    "acorn": "8.15.0",
    "acorn-jsx": "5.3.2",
    "angular-estree-parser": "14.1.0",
    "angular-html-parser": "10.1.1",
    "camelcase": "9.0.0",
    "ci-info": "4.3.1",
    "cjk-regex": "3.4.0",
    "collapse-white-space": "1.0.6",
    "css-units-list": "2.1.0",
    "dashify": "2.0.0",
    "deno-path-from-file-url": "0.0.4",
    "diff": "8.0.2",
    "editorconfig-without-wasm": "0.0.4",
    "emoji-regex": "10.6.0",
    "escape-string-regexp": "5.0.0",
    "espree": "11.0.0",
    "fast-glob": "3.3.3",
    "fast-json-stable-stringify": "2.1.0",
    "file-entry-cache": "11.1.1",
    "find-cache-directory": "6.0.0",
    "flow-parser": "0.291.0",
    "get-east-asian-width": "1.4.0",
    "get-stdin": "9.0.0",
    "graphql": "16.12.0",
    "hermes-parser": "0.32.1",
    "html-element-attributes": "3.5.0",
    "html-ua-styles": "0.2.0",
    "ignore": "7.0.5",
    "import-meta-resolve": "4.2.0",
    "index-to-position": "1.2.0",
    "is-es5-identifier-name": "1.0.1",
    "jest-docblock": "30.2.0",
    "json5": "2.2.3",
    "leven": "4.1.0",
    "linguist-languages": "9.1.11",
    "meriyah": "7.0.0",
    "micromatch": "4.0.8",
    "minimist": "1.2.8",
    "n-readlines": "3.4.0",
    "outdent": "0.8.0",
    "oxc-parser": "0.102.0",
    "parse-json": "8.3.0",
    "picocolors": "1.1.1",
    "please-upgrade-node": "3.2.0",
    "postcss": "8.5.6",
    "postcss-less": "6.0.0",
    "postcss-media-query-parser": "0.2.3",
    "postcss-scss": "4.0.9",
    "postcss-selector-parser": "2.2.3",
    "postcss-values-parser": "2.0.1",
    "regexp-util": "2.0.3",
    "remark-footnotes": "2.0.0",
    "remark-math": "3.0.1",
    "remark-parse": "8.0.3",
    "sdbm": "3.0.0",
    "search-closest": "1.1.0",
    "smol-toml": "1.5.2",
    "strip-ansi": "7.1.2",
    "to-fast-properties": "4.0.0",
    "trim-newlines": "5.0.0",
    "typescript": "5.9.3",
    "unicode-regex": "4.2.0",
    "unified": "9.2.2",
    "url-or-path": "2.6.1",
    "vnopts": "2.0.3",
    "wcwidth.js": "2.0.0",
    "yaml": "1.10.2",
    "yaml-unist-parser": "2.2.1"
  },
  "devDependencies": {
    "@babel/generator": "8.0.0-beta.3",
    "@eslint/js": "9.39.1",
    "@stylistic/eslint-plugin": "5.6.1",
    "@types/jsesc": "3.0.3",
    "@typescript-eslint/eslint-plugin": "8.48.1",
    "@typescript-eslint/parser": "8.48.1",
    "base64-arraybuffer-es6": "3.1.0",
    "browserslist": "4.28.0",
    "browserslist-to-esbuild": "2.1.1",
    "buffer": "6.0.3",
    "c8": "10.1.3",
    "commonmark-test-suite": "0.1.0",
    "cross-env": "10.1.0",
    "cspell": "9.3.2",
    "enquirer": "2.4.1",
    "esbuild": "0.27.0",
    "esbuild-visualizer": "0.7.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-formatter-friendly": "7.0.0",
    "eslint-plugin-compat": "6.0.2",
    "eslint-plugin-jest": "29.2.1",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-regexp": "2.10.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "62.0.0",
    "esm-utils": "4.4.2",
    "gfm-test-suite": "0.0.2",
    "globals": "16.5.0",
    "jest": "30.2.0",
    "jest-light-runner": "0.7.10",
    "jest-snapshot-serializer-ansi": "2.2.1",
    "jest-snapshot-serializer-raw": "2.0.0",
    "jest-watch-typeahead": "3.0.1",
    "jsesc": "3.1.0",
    "json-test-suite": "1.0.0",
    "knip": "5.73.1",
    "magic-string": "0.30.21",
    "make-synchronized": "0.8.0",
    "nano-spawn": "2.0.0",
    "node-style-text": "2.1.2",
    "npm-run-all2": "8.0.4",
    "open-editor": "6.0.0",
    "prettier": "3.7.4",
    "pretty-bytes": "7.1.0",
    "pretty-ms": "9.3.0",
    "rollup-plugin-license": "3.6.0",
    "semver": "7.7.3",
    "serialize-javascript": "7.0.2",
    "tempy": "3.1.0",
    "tinybench": "6.0.0",
    "ts-expect": "1.3.0",
    "yaml-test-suite": "0.1.0"
  },
  "resolutions": {
    "trim": "1.0.1"
  },
  "scripts": {
    "prepublishOnly": "echo \"Error: must publish from dist/\" && exit 1",
    "test": "jest",
    "test:node14": "cross-env NODE_OPTIONS=\"--experimental-loader=$PWD/scripts/nodejs-14-test-loader.js --no-warnings=ExperimentalWarning\" jest --runInBand",
    "test:dev-package": "cross-env INSTALL_PACKAGE=1 yarn test",
    "test:production": "cross-env NODE_ENV=production yarn test",
    "test:production-node14": "cross-env NODE_ENV=production yarn test:node14",
    "test:production-standalone": "cross-env TEST_STANDALONE=1 yarn test:production",
    "test:production-standalone-node14": "cross-env TEST_STANDALONE=1 yarn test:production-node14",
    "test:production-lint": "eslint dist/prettier --config=./scripts/bundle-eslint-config.js --quiet --format friendly",
    "perf": "yarn && yarn build && cross-env NODE_ENV=production node ./dist/prettier/bin/prettier.cjs",
    "perf:inspect": "yarn && yarn build && cross-env NODE_ENV=production node --inspect-brk ./dist/prettier/bin/prettier.cjs",
    "perf:benchmark": "yarn perf --debug-benchmark",
    "perf:compare": "./scripts/benchmark/compare.sh",
    "lint": "run-p --continue-on-error \"lint:*\"",
    "lint:typecheck": "tsc",
    "lint:eslint": "cross-env EFF_NO_LINK_RULES=true eslint . --format friendly",
    "lint:changelog": "node ./scripts/lint-changelog.js",
    "lint:prettier": "prettier . --check --cache",
    "lint:spellcheck": "cspell --no-progress --relative --dot --gitignore",
    "lint:deps": "node ./scripts/check-deps.js",
    "lint:knip": "knip",
    "lint:format-test": "node ./scripts/format-test-lint.js",
    "fix": "run-s --continue-on-error fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "build": "node ./scripts/build/build.js",
    "build:website": "node ./scripts/build-website.js",
    "build:website:pr": "cross-env PULL_REQUEST=true yarn build:website",
    "generate-changelog": "node ./scripts/generate-changelog.js",
    "debug": "node bin/prettier.js --ignore-path=.prettierignore --plugin=./packages/plugin-oxc/index.js --plugin=./packages/plugin-hermes/index.js",
    "debug:watch": "node --watch bin/prettier.js --ignore-path=.prettierignore --plugin=./packages/plugin-oxc/index.js --plugin=./packages/plugin-hermes/index.js",
    "debug:inspect": "node --inspect-brk bin/prettier.js --ignore-path=.prettierignore --plugin=./packages/plugin-oxc/index.js --plugin=./packages/plugin-hermes/index.js"
  },
  "c8": {
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "include": [
      "src/**",
      "bin/**"
    ],
    "exclude": [
      "bin/prettier.js",
      "src/standalone.js",
      "src/index.cjs",
      "src/document/debug.js",
      "src/universal/*.browser.js",
      "src/utilities/unexpected-node-error.js",
      "src/language-js/types/estree.d.ts",
      "src/**/**/*.d.ts"
    ]
  },
  "browserslist": [
    ">0.5%",
    "not dead",
    "not op_mini all"
  ],
  "preferUnplugged": true,
  "packageManager": "yarn@4.12.0",
  "sideEffects": false
}
