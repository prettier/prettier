    "@babel/code-frame": "7.14.5",
    "@babel/parser": "7.14.7",
    "@brodybits/remark-parse": "8.0.5",
    "@glimmer/syntax": "0.80.0",
    "@iarna/toml": "2.2.5",
    "@typescript-eslint/typescript-estree": "4.28.2",
