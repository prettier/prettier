{
  "name": "prettierx",
  "version": "0.11.1-dev",
  "description": "prettierx - less opinionated code formatter fork of prettier",
  "bin": {
    "prettierx": "./bin/prettierx.js"
  },
  "repository": "brodybits/prettierx",
  "homepage": "https://github.com/brodybits/prettierx",
  "author": "James Long",
  "license": "MIT",
  "main": "./index.js",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "bin",
    "index.js",
    "lib",
    "src"
  ],
  "dependencies": {
    "@angular/compiler": "8.2.13",
    "@babel/code-frame": "7.5.5",
    "@babel/parser": "7.7.3",
    "@glimmer/syntax": "0.41.0",
    "@iarna/toml": "2.2.3",
