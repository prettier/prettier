{
  "name": "prettier",
  "version": "0.0.3",
  "bin": {
    "prettier": "./bin/prettier.js"
  },
  "main": "./index.js",
  "dependencies": {
    "ast-types": "git+https://github.com/jlongster/ast-types.git",
    "babylon": "^6.15.0",
    "esutils": "^2.0.2",
    "flow-parser": "^0.37.0",
    "minimist": "^1.2.0",
    "private": "^0.1.6",
    "recast": "^0.11.18",
    "source-map": "^0.5.6"
  },
  "devDependencies": {
    "jest": "^18.0.0"
  },
  "scripts": {
    "test": "jest",
    "format": "./bin/prettier.js --write",
    "format:single": "npm run format -- src/printer.js",
    "format:all": "npm run format -- src/*.js bin/*.js"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests_config/run_spec.js"
    ],
    "testRegex": "jsfmt\\.spec\\.js$",
    "testPathIgnorePatterns": [
      "tests/new_react",
      "tests/more_react"
    ]
  }
}
