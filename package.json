    "please-upgrade-node": "3.2.0",
    "postcss-less": "3.1.4",
    "postcss-media-query-parser": "0.2.3",
    "postcss-scss": "2.1.1",
    "postcss-selector-parser": "2.2.3",
    "postcss-values-parser": "2.0.1",
    "regexp-util": "1.2.2",
    "remark-footnotes": "1.0.0",
    "remark-math": "1.0.6",
    "remark-parse": "8.0.2",
    "resolve": "1.17.0",
    "semver": "7.3.2",
    "string-width": "4.2.0",
    "tslib": "1.13.0",
    "typescript": "3.9.5",
