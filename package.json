{
  "name": "prettier",
  "version": "2.1.0-dev",
  "description": "Prettier is an opinionated code formatter",
  "bin": "./bin/prettier.js",
  "repository": "prettier/prettier",
  "homepage": "https://prettier.io",
  "author": "James Long",
  "license": "MIT",
  "main": "./index.js",
  "browser": "./standalone.js",
  "unpkg": "./standalone.js",
  "engines": {
    "node": ">=10.13.0"
  },
  "files": [
    "index.js",
    "standalone.js",
    "src",
    "bin"
  ],
  "dependencies": {
