{
  "name": "prettier",
  "version": "3.0.0-alpha.4",
  "description": "Prettier is an opinionated code formatter",
  "bin": "./bin/prettier.cjs",
  "repository": "prettier/prettier",
  "funding": "https://github.com/prettier/prettier?sponsor=1",
  "homepage": "https://prettier.io",
  "author": "James Long",
  "type": "module",
  "license": "MIT",
  "main": "./src/index.cjs",
  "browser": "./standalone.js",
  "unpkg": "./standalone.js",
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "default": "./src/index.js"
    },
    "./*": "./*"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "index.js",
    "standalone.js",
    "src",
    "bin"
  ],
  "dependencies": {
    "@angular/compiler": "15.0.1",
    "@babel/code-frame": "7.18.6",
    "@babel/parser": "7.20.3",
    "@babel/types": "7.20.2",
    "@glimmer/syntax": "0.84.2",
    "@iarna/toml": "2.2.5",
    "@prettier/is-es5-identifier-name": "0.1.0",
    "@prettier/parse-srcset": "2.0.2",
    "@typescript-eslint/typescript-estree": "5.44.0",
    "@typescript-eslint/visitor-keys": "5.44.0",
    "acorn": "8.8.1",
    "acorn-jsx": "5.3.2",
    "angular-estree-parser": "5.0.1",
    "angular-html-parser": "3.0.0",
    "camelcase": "7.0.0",
    "chalk": "5.1.2",
    "ci-info": "3.7.0",
    "cjk-regex": "2.0.1",
    "cosmiconfig": "8.0.0",
    "css-units-list": "2.0.1",
    "dashify": "2.0.0",
    "diff": "5.1.0",
    "editorconfig": "0.15.3",
    "editorconfig-to-prettier": "0.2.0",
    "escape-string-regexp": "5.0.0",
    "espree": "9.4.1",
    "fast-glob": "3.2.12",
    "fast-json-stable-stringify": "2.1.0",
    "file-entry-cache": "6.0.1",
    "find-cache-dir": "4.0.0",
    "find-parent-dir": "0.3.1",
    "flow-parser": "0.194.0",
    "get-stdin": "9.0.0",
    "graphql": "16.6.0",
    "hermes-eslint": "0.9.0",
    "html-element-attributes": "3.1.0",
    "html-styles": "1.0.0",
    "html-tag-names": "2.0.1",
    "html-void-elements": "2.0.1",
    "ignore": "5.2.1",
    "import-meta-resolve": "2.2.0",
    "jest-docblock": "29.2.0",
    "json5": "2.2.1",
    "leven": "4.0.0",
    "lines-and-columns": "2.0.3",
    "linguist-languages": "7.21.0",
    "mem": "9.0.2",
    "meriyah": "4.3.3",
    "micromatch": "4.0.5",
    "minimist": "1.2.7",
    "n-readlines": "1.0.1",
    "outdent": "0.8.0",
    "please-upgrade-node": "3.2.0",
    "postcss": "8.4.19",
    "postcss-less": "6.0.0",
    "postcss-media-query-parser": "0.2.3",
    "postcss-scss": "4.0.6",
    "postcss-selector-parser": "2.2.3",
    "postcss-values-parser": "2.0.1",
    "regexp-util": "1.2.2",
    "remark-footnotes": "2.0.0",
    "remark-math": "3.0.1",
    "remark-parse": "8.0.3",
    "sdbm": "2.0.0",
    "string-width": "5.0.1",
    "strip-ansi": "7.0.1",
    "to-fast-properties": "4.0.0",
    "typescript": "4.9.3",
    "unicode-regex": "3.0.0",
    "unified": "9.2.2",
    "vnopts": "1.0.2",
    "wcwidth": "1.0.1",
    "yaml-unist-parser": "2.0.1"
  },
  "devDependencies": {
    "@babel/generator": "7.20.4",
    "@esbuild-plugins/node-modules-polyfill": "0.1.4",
    "@glimmer/reference": "0.84.2",
    "@types/estree": "1.0.0",
    "@types/file-entry-cache": "5.0.2",
    "@types/find-cache-dir": "3.2.1",
    "@types/jest": "29.2.3",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "benchmark": "2.1.4",
    "browserslist-to-esbuild": "1.2.0",
    "c8": "7.12.0",
    "cross-env": "7.0.3",
    "cspell": "6.15.0",
    "enquirer": "2.3.6",
    "esbuild": "0.15.16",
    "esbuild-visualizer": "0.3.1",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-formatter-friendly": "7.0.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.6",
    "eslint-plugin-n": "15.5.1",
    "eslint-plugin-prettier-internal-rules": "2.0.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-regexp": "1.11.0",
    "eslint-plugin-unicorn": "45.0.0",
    "esm-utils": "4.1.1",
    "execa": "6.1.0",
    "jest": "29.3.1",
    "jest-light-runner": "0.4.1",
    "jest-snapshot-serializer-ansi": "1.0.0",
    "jest-snapshot-serializer-raw": "1.2.0",
    "jest-watch-typeahead": "2.2.1",
    "magic-string": "0.26.7",
    "npm-run-all": "4.1.5",
    "path-browserify": "1.0.1",
    "prettier": "2.8.0",
    "pretty-bytes": "6.0.0",
    "rollup-plugin-license": "3.0.1",
    "semver": "7.3.8",
    "serialize-javascript": "6.0.0",
    "snapshot-diff": "0.10.0",
    "tempy": "3.0.0"
  },
  "resolutions": {
    "trim": "1.0.1"
  },
  "scripts": {
    "prepublishOnly": "echo \"Error: must publish from dist/\" && exit 1",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:dev-package": "cross-env INSTALL_PACKAGE=1 yarn test",
    "test:dist": "cross-env NODE_ENV=production yarn test",
    "test:dist-standalone": "cross-env TEST_STANDALONE=1 yarn test:dist",
    "test:dist-lint": "eslint --no-eslintrc --no-ignore --no-inline-config --config=./scripts/bundle-eslint-config.cjs \"dist/**/*.{js,cjs,mjs}\"",
    "perf": "yarn && yarn build && cross-env NODE_ENV=production node ./dist/bin-prettier.js",
    "perf:inspect": "yarn && yarn build && cross-env NODE_ENV=production node --inspect-brk ./dist/bin-prettier.js",
    "perf:benchmark": "yarn perf --debug-benchmark",
    "perf:compare": "./scripts/benchmark/compare.sh",
    "lint": "run-p \"lint:*\"",
    "lint:typecheck": "tsc",
    "lint:eslint": "cross-env EFF_NO_LINK_RULES=true eslint . --format friendly",
    "lint:changelog": "node ./scripts/lint-changelog.mjs",
    "lint:prettier": "prettier . \"!test*\" --check",
    "lint:spellcheck": "cspell --no-progress --relative --dot --gitignore",
    "lint:deps": "node ./scripts/check-deps.mjs",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "build": "node ./scripts/build/build.mjs",
    "build:website": "node ./scripts/build-website.mjs"
  },
  "c8": {
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "include": [
      "src/**",
      "bin/**"
    ],
    "exclude": [
      "bin/prettier.js",
      "src/standalone.js",
      "src/index.cjs",
      "src/document/debug.js",
      "src/utils/unexpected-node-error.js",
      "src/language-js/types/estree.d.ts"
    ]
  },
  "browserslist": [
    ">0.5%",
    "not ie 11",
    "not safari 5.1",
    "not op_mini all"
  ],
  "packageManager": "yarn@3.3.0"
}
